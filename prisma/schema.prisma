generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @map("_id") @default(auto()) @db.ObjectId
  name  String?
  email String? @unique
}

model Account {
  id                String  @id @map("_id") @default(auto()) @db.ObjectId
  userId            String
  provider          String
  providerAccountId String
}

model Session {
  id           String @id @map("_id") @default(auto()) @db.ObjectId
  sessionToken String @unique
  userId       String
}
